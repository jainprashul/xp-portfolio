{
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "/api/:path*"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "functions": {
      "api/**/*.js": {
        "memory": 128,
        "maxDuration": 10
      }
    }
  }